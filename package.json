{
  "name": "simphony-remote-deps",
  "version": "1.2.0",
  "description": "SimPhoNy-remote nodejs dependencies",
  "author": "Enthought",
  "license": "BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/simphony-remote/simphony-remote.git"
  },
  "scripts": {
    "build": "browserify -t [ babelify --presets [ react ] ] remoteappmanager/static/js/home/react-input.js -o build/lib/remoteappmanager/static/js/home/react-output.js"
  },
  "dependencies": {
    "bower": "*",
    "configurable-http-proxy": "git://github.com/jupyterhub/configurable-http-proxy.git#2.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "jshint": "*",
    "node-qunit-phantomjs": "*"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
